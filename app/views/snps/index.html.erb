<h1>Listing all SNPs</h1>
<table class="common-table zebra-striped">
<tr>
	<th>SNP-ID</th>
	<th>Name</th>
	<th>Position</th>
	<th>Chromosome</th>
	<th>Genotype-Frequency</th>
	<th>Allele-Frequency</th>
</tr>
<% @snps.each do |s| %>
	<td><%= s.id %></td>
	<td><%= link_to(s.name, s) %></td>
	<td><%= s.position %></td>
	<td><%= s.chromosome %></td>
	<td><%= s.genotype_frequency.keys()[0] + " = " + s.genotype_frequency.values()[0].to_s %></td>
	<td><% s.allele_frequency.each do |k,v| %><%= k + " = " + v.to_s + " "  %><% end %></td>
</tr>
<% end %>
</table>
<%= will_paginate(@snps) %>


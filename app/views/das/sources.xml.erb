<SOURCES>
<% @users.each do |u| %>
    <SOURCE uri="<%= u.id %>"
            title="<%= u.id %>"
            doc_href="<%= url_for(:controller => :users, :method => "show", :id => u.id) %>"
            description="<%= u.id %>">
            <MAINTAINER email="admin@opensnp.org"/>
            <VERSION created="<%=u.created_at%>" uri="<%= u.id %>">
                <COORDINATES uri="http://www.dasregistry.org/dasregistry/coordsys/CS_DS40" source="Chromosome" authority="NCBI" taxid="9606" version="36" test_range="1:0,1">
                        NCBI_36,Chromosome,Homo sapiens
                </COORDINATES>

        
            </VERSION>
        </SOURCE>
<% end %>
</SOURCES>

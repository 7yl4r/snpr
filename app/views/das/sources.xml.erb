<SOURCES>
<% @users.each do |u| %>
	<% if u.genotypes != []%>
    <SOURCE uri="<%= u.id %>" title="<%= u.id %>" description="<%= u.id %>">
            <MAINTAINER email="admin@opensnp.org"/>
            <VERSION created="<%=u.created_at%>" uri="<%= u.id %>">
                <COORDINATES uri="http://www.dasregistry.org/dasregistry/coordsys/CS_DS40" source="Chromosome" authority="NCBI" taxid="9606" version="36" test_range="1:0,1">NCBI_36,Chromosome,Homo sapiens</COORDINATES>
                <CAPABILITY query_uri="<%= root_url+"das/"+u.id.to_s+"/features"%>" type="das1:features"/>
				<CAPABILITY type="das1:unknown-segment"/>
                <PROP value="valid" name="features"/>
            </VERSION>
        </SOURCE>
	<%end%>
<% end %>
</SOURCES>
